{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"typeRoots": ["./node_modules/@types", "./src/@types"],
		"paths": {
			"@features/*": ["./src/features/*"],
			"@config/*": ["./src/config/*"],
			"@navigation/*": ["./src/navigation/*"],
			"@theme/*": ["./src/theme/*"],
			"@assets/*": ["./src/assets/*"],
			"@app-types": ["./src/types/app-types"]
		}
	}
}
